{
  "name": "react-cordova-boilerplate",
  "version": "1.2.0",
  "description": "TodoMVC example for react with development tools to build a cordova application",
  "repository": {
    "type": "git",
    "url": "https://github.com/unimonkiez/react-cordova-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "flux",
    "redux",
    "todomvc"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unimonkiez/react-cordova-boilerplate/issues"
  },
  "enviroments": {
    "__DEV__": {
      "__SSR__": false,
      "__DEVTOOLS__": true
    },
    "__PROD__": {
      "__SSR__": true,
      "__DEVTOOLS__": false
    },
    "__CORDOVA__": {
      "__SSR__": true,
      "__DEVTOOLS__": true
    }
  },
  "homepage": "https://github.com/unimonkiez/react-cordova-boilerplate#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "classnames": "^2.1.2",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "deasync": "^0.1.2",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.2.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.16.0",
    "imports-loader": "^0.6.5",
    "install": "^0.4.1",
    "jasmine": "^2.3.2",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.9",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-webpack": "^1.7.0",
    "memory-fs": "^0.3.0",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.3.3",
    "npm": "^3.5.1",
    "phantomjs": "^1.9.18",
    "radium": "^0.15.3",
    "raw-loader": "^0.5.1",
    "react": "^0.14",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.4.0",
    "redux": "^3.0.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.9.0"
  },
  "scripts": {
    "build": "webpack --progress",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:cordova": "cross-env NODE_ENV=production cross-env BUILD_TARGET=cordova npm run build",
    "start": "webpack-dev-server --port 3000 --inline --hot  --progress --colors ",
    "start:prod": "cross-env NODE_ENV=production npm run start",
    "lint": "eslint --ext .js,.jsx .",
    "test": "karma start",
    "predeploy": "npm run build:cordova",
    "deploy": "git add -f www/ && node ./bin/bump.js && git checkout gh-pages && git merge master && git checkout master && git push --tags && npm publish && git push origin master gh-pages"
  }
}
