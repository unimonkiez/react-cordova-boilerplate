{
  "name": "react-cordova-boilerplate",
  "version": "1.0.2",
  "description": "TodoMVC example for react with development tools to build a cordova application",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuvalsaraf/react-cordova-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "flux",
    "redux",
    "todomvc"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuvalsaraf/react-cordova-boilerplate/issues"
  },
  "enviroments": {
    "__DEV__": {
      "__MINIFICATION__": false,
      "__SOURCEMAP__": true,
      "__SSR__": false,
      "__DEVTOOLS__": true
    },
    "__PROD__": {
      "__MINIFICATION__": true,
      "__SOURCEMAP__": false,
      "__SSR__": true,
      "__DEVTOOLS__": false
    },
    "__CORDOVA__": {
      "__MINIFICATION__": true,
      "__SOURCEMAP__": false,
      "__SSR__": true,
      "__DEVTOOLS__": false
    }
  },
  "homepage": "https://github.com/yuvalsaraf/react-cordova-boilerplate#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.22",
    "babel-eslint": "^4.1.1",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.25",
    "classnames": "^2.1.2",
    "css-loader": "^0.19.0",
    "deasync": "^0.1.2",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.6.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "history": "^1.9.1",
    "html-webpack-plugin": "^1.6.1",
    "jasmine": "^2.3.2",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.9",
    "karma-jasmine": "^0.3.6",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-webpack": "^1.7.0",
    "memory-fs": "^0.2.0",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.3.3",
    "phantomjs": "^1.9.18",
    "radium": "^0.14.1",
    "raw-loader": "^0.5.1",
    "react": "^0.14",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc1",
    "redux": "^3.0.0",
    "redux-devtools": "^2.1.5",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.9.0"
  },
  "scripts": {
    "build": "webpack -d --config config/webpack.config.js",
    "build:prod": "webpack -p --config config/webpack.config.js",
    "build:cordova": "webpack -c --config config/webpack.config.js",
    "start": "node config/server.js -d",
    "start:prod": "node config/server.js -p",
    "lint": "eslint -c config/.eslintrc --ext .js,.jsx ./src",
    "test": "karma start config/karma.conf.js"
  }
}
