  "scripts": {
    "build": "node ./bin/build.js",
    "build:watch": "npm run build -- -w",
    "build:prod": "npm run build -- -p",
    "build:prod:watch": "npm run build -- -p -w",
    "build:watch:prod": "npm run build:prod:watch",
    "start": "node ./bin/start.js",
    "start:prod": "node ./bin/start.js -- -p",
    "lint": "eslint --fix --ext .js,.jsx .",
    "lint:report": "npm run lint -- --format html > ./report.html",
    "test": "node ./bin/test.js",
    "test:coveralls": "npm test -- -coverageToLcov",
    "test:watch": "npm test -- -w",
    "test:coverage": "npm test -- -coverage",
    "test:coverage:watch": "npm test -- -coverage -w",
    "test:watch:coverage": "npm run test:coverage:watch",
    "predeploy": "npm run build",
    "deploy": "git add -f www/ && node ./bin/bump.js && git checkout gh-pages && git merge master && git commit -m 'Merge from master' && git checkout master && git push --tags && npm publish && git push origin master gh-pages"
